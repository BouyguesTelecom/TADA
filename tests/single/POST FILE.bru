meta {
  name: POST FILE
  type: http
  seq: 3
}

post {
  url: {{URL_API}}{{URL_API_PREFIX}}/file
  body: multipartForm
  auth: bearer
}

auth:bearer {
  token: {{AUTH_TOKEN}}
}

body:multipart-form {
  namespace: DEV
  file: @file(/Users/amlouler/Downloads/test.webp)
  ~destination: 
  ~expired: true
}

vars:post-response {
  IMAGE_URL: res.body.data[0].public_url
  IMAGE_ID: res.body.data[0].uuid
}

assert {
  res.status: eq 200
}
