meta {
  name: UPLOAD FILES
  type: http
  seq: 1
}

post {
  url: {{URL_API}}{{URL_API_PREFIX}}/files
  body: multipartForm
  auth: bearer
}

auth:bearer {
  token: {{AUTH_TOKEN}}
}

body:multipart-form {
  namespace: DEV
  files: @file(/tmp/images/test.pdf|/tmp/images/test.svg)
  ~toWebp: false
}
