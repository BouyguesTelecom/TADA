meta {
  name: 8 - PATCH FILE with FILE
  type: http
  seq: 7
}

patch {
  url: {{URL_API}}{{URL_API_PREFIX}}/file/{{IMAGE_ID}}
  body: multipartForm
  auth: bearer
}

auth:bearer {
    token: {{AUTH_TOKEN}}
}

body:json {
  {
    "namespace": "DEV",
    "information": "lol"
  }
}

body:multipart-form {
  namespace: DEV
  information: update
  file: @file(/tmp/images/test.png)
  toWebp: false
}

assert {
  res.body.errors[0]: eq Mimetypes are not the same or conversion not allowed
}
