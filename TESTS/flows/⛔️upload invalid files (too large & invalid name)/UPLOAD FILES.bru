meta {
  name: UPLOAD FILES
  type: http
  seq: 1
}

post {
  url: {{URL_API}}{{URL_API_PREFIX}}/files
  body: multipartForm
  auth: none
}

body:multipart-form {
  namespace: DEV
  files: @file(/tmp/images/test_too_large.jpg|/tmp/images/test name forbidden.jpg)
  ~toWebp: false
}

assert {
  res.status: eq 200
  res.body.data: length 0
  res.body.errors: length 2
}
