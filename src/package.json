        "sharp": "^0.33.4",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "typescript": "5.8.2",
        "uuid": "^10.0.0",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.5",
